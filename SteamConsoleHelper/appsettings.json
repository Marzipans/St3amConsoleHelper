{
  "Features": {
    // Allowed types: 'Telegram', 'Console'
    "Messenger": "Telegram",

    // Allowed types: 'None', 'File'
    "DataStorage": "File"
  },

  "Telegram:ApiToken": "<enter your telegram api key here>",

  // Do not change lines below
  "Serilog": {
    "IncludeScopes": false,
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] [{SourceContext}]: {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ]
}
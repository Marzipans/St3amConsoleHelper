{
  "FakeServices": {
    "SteamAuthenticationService": {
      "IsEnabled": false,
      "Data": {
        "SteamID": "Type your steam id here",
        "SessionID": "Type your SessionId from steamcommunity cookies here",
        "SteamLoginSecure": "Type your SteamLoginSecure from steamcommunity cookies here"
      }
    }
  },
  "Telegram": {
    "AuthorUsername": "taigadev"
  },
  "Serilog": {
    "IncludeScopes": false,
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u3}] [{SourceContext}]: {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ]
}